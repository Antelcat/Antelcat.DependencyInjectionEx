<Project>
    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>8.0.0</Version>
        <Authors>Antelcat</Authors>
        <Copyright>Copyright Antelcat. All rights reversed</Copyright>
        <PackageProjectUrl>https://github.com/Antelcat/Antelcat.DependencyInjectionEx</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Antelcat/Antelcat.DependencyInjectionEx.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageIcon>Icon.png</PackageIcon>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageReleaseNotes>Initial</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\..\docs\Icon.png">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
            <Link>Icon.png</Link>
        </None>
        <None Include="..\..\..\README.md">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
            <Link>README.md</Link>
        </None>
    </ItemGroup>
</Project>